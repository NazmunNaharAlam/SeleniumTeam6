<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="BankOfAmerica Automation Test" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="mac"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="53.0"/>
    <parameter name="url" value="https://www.bankofamerica.com/"/>

    <test name="BankOfAmerica Home">
        <classes>
            <class name="testBOAhome.TestLogin"/>
            <class name="page.SearchElements"/>
            <!--<class name="search.DropDownMenuSearch"/>-->
        </classes>
    </test>

</suite>